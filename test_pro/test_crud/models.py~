from django.db import models

# Create your models here.

class IMAGE_INTENSIFICATION_BASED_DEVICES(models.Model):
	DEVICE_NAME		= models.CharField(max_length=150, primary_key=True)
	MGNIFICATION		= models.CharField(max_length=30)
	FIELD_OF_VIEW		= models.CharField(max_length=30)
	RANGE			= models.CharField(max_length=30)
